package exam;

public class Exam07 {

	public static void main(String[] args) {
		
		int[] lotto = new int[5];
		lotto[] = 0;
		
		for (int i =0; i<lotto.length ; i++) {
			lotto[i] = (int)(Math.random()*45)+1;
			if(lotto[i] == lotto[i+1])
				lotto[i+1] = (int)(Math.random()*45)+1;
		
		}
		
		for (int i=0; i<lotto.length; i++) System.out.println(i);
		
	}

}
