package exam;

/*
 * +   1
 * ++  12
 * +++ 123
 * ++++ 1234
 * +++++ 12345
 */
public class Exam03 {
	public static void main(String[] args) {
		
		String str = "";
		
		for(int i=0; i<5 ; i++) {
		
			for(int j=0; j<i+1 ; j++) {
				System.out.printf("%5s%n", str);
				System.out.print("+");  
			}
			//System.out.print("\n");
		}
	}
}
