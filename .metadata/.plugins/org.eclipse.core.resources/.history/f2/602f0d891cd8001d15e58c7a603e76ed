package exam;

/*
 * String str = "12345"
 * 
 * charAt(0) -> int
 */

public class Exam07 {

	public static void main(String[] args) {

		String str = "123426";
		int sum = 0;
		int test = 0;
		
		for(int i=0; i< str.length(); i++) {			
			test =	Integer.parseInt(str, str.charAt(i));
			System.out.println(test);
			
			//			sum = sum + (str.charAt(i) - '0');	
		}
		System.out.println("sum=" + sum);
	}

}
