package exam;

/*
 * String str = "12345"
 * 
 * charAt(0) -> int
 */

public class Exam07 {

	public static void main(String[] args) {

		String str = "12345";
		int sum = 0;
		
		for(int i=0; i< str.length(); i++) {			
			sum = sum + (str.charAt(i) - '0');
			
		//	System.out.println(str.charAt(i) - '0');

			
		}
		System.out.println("sum=" + sum);
	}

}
