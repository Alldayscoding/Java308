package exam;


import java.lang.reflect.Array;
import java.util.Arrays;

public class Exam05 {

	public static void main(String[] args) {
	
	final int SIZE = 5;
	final int MAX = 100;	
	final int MIN = 10;
	
	int max, min; max = min = 0;
	int[] numArray = new int[SIZE];
	
	for (int i = 0; i<numArray.length; i++ ) {
		if (numArray[i] > max) max = numArray[i];
		else min = numArray[i];
	} 
	
	
	}

}
