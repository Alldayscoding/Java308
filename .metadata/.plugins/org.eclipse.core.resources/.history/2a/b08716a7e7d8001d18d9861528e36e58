package exam;
public class Exam06 {

	public static void main(String[] args) {
		final int SIZE = 5;
		final int MAX = 50;	
		final int MIN = 10;
		
		
		int[] arr = new int[] {3,7,2};   // 2 3 7로 나와야함
			for (int i = 0; i<arr.length; i++)
				for (int j=0; j<arr.length; j++) {
					if(arr[i] > arr[j]) arr[i] = arr[j];
				}
			System.out.println(arr[2]);
	}
	
}


