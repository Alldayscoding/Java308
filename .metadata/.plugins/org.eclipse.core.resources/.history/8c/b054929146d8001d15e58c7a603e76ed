package exam;

import java.util.Scanner;

public class Exam11 {

	public static void main(String[] args) {
		
		int qty = 0;
		int sum = 0;
		
		Scanner sc = new Scanner(System.in);
		System.out.print("몇 개의 숫자를 입력할꺼니?>");
		qty = sc.nextInt();//3
		
		while(qty != 0) {
			for (int i=0; i<qty; i++ ) {
				System.out.print(i+1 + "번째 숫자를 입력하세요>");
				
				
				
			}
			
			
		}
		
		
	 }
}
